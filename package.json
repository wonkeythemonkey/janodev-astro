{
  "name": "@janodev/astro",
  "version": "0.1.0-pre1.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "cz": "cz",
    "changelog": "conventional-changelog -c .conventional-changelog/config.json -i CHANGELOG.md -s",
    "prepare": "husky install",
    "lint": "eslint .",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/tailwind": "^6.0.2",
    "@resvg/resvg-js": "^2.6.2",
    "astro": "^5.12.5",
    "astro-expressive-code": "^0.40.2",
    "astro-robots-txt": "^1.0.0",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.13",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.2",
    "@divriots/jampack": "^0.24.4",
    "@tailwindcss/typography": "^0.5.13",
    "@types/github-slugger": "^1.3.0",
    "@typescript-eslint/parser": "^7.13.1",
    "accessible-astro-components": "^2.3.6",
    "astro-eslint-parser": "^1.0.2",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint-plugin-astro": "^1.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "marked": "^13.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
